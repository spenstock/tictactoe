{
    "name": "TicTacToe",
    "version": "1.1",
    "description": "Popup TicTacToe!",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/x_16.png",
        "32": "images/x_32.png",
        "48": "images/x_48.png",
        "128": "images/x_128.png"
      }
    },
    "icons": {
      "16": "images/x_16.png",
      "32": "images/x_32.png",
      "48": "images/x_48.png",
      "128": "images/x_128.png"
    },
    "manifest_version": 2
  }